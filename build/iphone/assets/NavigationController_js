exports.NavigationController=function(){this.windowStack=[]},exports.NavigationController.prototype.open=function(e){this.windowStack.push(e),e.navBarHidden=e.navBarHidden||!1;if(this.windowStack.length===1)if(Ti.Platform.osname==="iphone"){this.navGroup=Ti.UI.iPhone.createNavigationGroup({window:e});var t=Ti.UI.createWindow();t.add(this.navGroup),t.open()}else if(Ti.Platform.osname==="mobileweb"){this.navGroup=Ti.UI.MobileWeb.createNavigationGroup({window:e});var t=Ti.UI.createWindow();t.add(this.navGroup),t.open()}else e.exitOnClose=!0,e.open();else Ti.Platform.osname==="iphone"?this.navGroup.open(e):Ti.Platform.osname==="mobileweb"?this.navGroup.open(e):e.open()},exports.NavigationController.prototype.close=function(){var e=this.windowStack.concat([]);windowToClose=e[e.length-1],this.navGroup?this.navGroup.close(windowToClose):windowToClose.close(),this.windowStack.pop()},exports.NavigationController.prototype.home=function(){for(var e=this.windowStack.length;e>2;e--){var t=this.windowStack.concat([]);windowToClose=t[t.length-1],this.navGroup?this.navGroup.close(windowToClose):windowToClose.close(),this.windowStack.pop()}}