function ApplicationWindow(){var self=Ti.UI.createWindow({backgroundColor:hubAPI.customBgColor,});LoginView=require('ui/common/LoginView');var loginView=new LoginView();var navGroup=Ti.UI.iPhone.createNavigationGroup({window:loginView});self.add(navGroup);this.openNewWindow=function(){navGroup.open(Ti.App.globalWindow);}
this.closeWindow=function(){navGroup.close(Ti.App.globalWindow);}
this.openMainWindow=function(){self.open();}};module.exports=ApplicationWindow;