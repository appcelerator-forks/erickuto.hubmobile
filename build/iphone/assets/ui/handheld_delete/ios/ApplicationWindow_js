function ApplicationWindow(){var e=Ti.UI.createWindow({}),t=Ti.UI.createWindow({backgroundColor:"black"}),n=Ti.UI.iPhone.createNavigationGroup({window:t});e.add(n),this.openNewWindow=function(e){n.open(e)},this.closeWindow=function(){n.close(Ti.App.globalWindow)},this.openMainWindow=function(){e.open(),LoginView=require("ui/common/LoginView");var t=new LoginView;n.open(t)}}module.exports=ApplicationWindow