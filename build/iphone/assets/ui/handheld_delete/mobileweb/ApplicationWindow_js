function ApplicationWindow(){var e=Ti.UI.createWindow({backgroundColor:"black"}),t=Ti.UI.createTab({window:e}),n=Ti.UI.createTabGroup();n.addTab(t),this.openNewWindow=function(e){Ti.UI.currentTab.open(e)},this.openMainWindow=function(){n.open(),LoginView=require("ui/common/LoginView");var e=new LoginView;Ti.UI.currentTab.open(e)},this.closeWindow=function(e){Ti.UI.currentTab.close(e)}}module.exports=ApplicationWindow