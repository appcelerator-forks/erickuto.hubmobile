function ApplicationWindow(){LoginView=require("ui/common/LoginView");var e=new LoginView;e.exitOnClose=!0,e.addEventListener("android:back",function(t){e.open()}),LoginView=require("ui/common/LoginView");var t=new LoginView;this.openNewWindow=function(){Ti.App.globalWindow.modal=!0,Ti.App.globalWindow.open()},this.openMainWindow=function(){e.open()},this.closeWindow=function(){Ti.App.globalWindow.close()}}module.exports=ApplicationWindow