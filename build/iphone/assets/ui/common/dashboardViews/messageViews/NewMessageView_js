function NewMessageView(e){hub=require("hub");var t=hub.API.hsf,n=hub.API.wsf,r=(hub.API.app_width-350*n)/2,i=function(){var e=Ti.UI.createView({top:10,backgroundColor:hub.API.customBgColor,layout:"vertical"});e.add(Ti.UI.createLabel({top:10,text:"To",left:5,color:hub.API.util.customTextColor,font:{fontSize:30*t,fontColor:hub.API.util.customTextColor,fontFamily:hub.API.util.customFont}}));var n=Ti.UI.createTextField({width:"97%",height:80*t,font:{fontSize:30*t,fontColor:hub.API.util.customTextColor,fontFamily:hub.API.util.customFont},hintText:"Enter name(s) here...",keyboardType:Ti.UI.KEYBOARD_DEFAULT,returnKeyType:Ti.UI.RETURNKEY_DEFAULT,borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,borderColor:"#e0e0e0",borderRadius:5,borderWidth:1});e.add(n),e.add(Ti.UI.createLabel({top:10,text:"Subject",left:5,color:hub.API.util.customTextColor,font:{fontSize:30*t,fontColor:hub.API.util.customTextColor,fontFamily:hub.API.util.customFont}}));var r=Ti.UI.createTextField({color:hub.API.util.customTextColor,width:"97%",height:80*t,font:{fontSize:20*t,fontFamily:hub.API.util.customFont},keyboardType:Ti.UI.KEYBOARD_DEFAULT,returnKeyType:Ti.UI.RETURNKEY_DEFAULT,borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,borderColor:"#e0e0e0",borderRadius:5,borderWidth:1});e.add(r),e.add(Ti.UI.createLabel({top:10,text:"Message",left:5,color:hub.API.util.customTextColor,font:{fontSize:30*t,fontColor:hub.API.util.customTextColor,fontFamily:hub.API.util.customFont}}));var i=Ti.UI.createTextArea({borderWidth:2,borderColor:"#bbb",borderRadius:5,color:"#888",font:{fontSize:15},textAlign:"left",hintText:"I am a textarea",top:0,left:5,width:"97%",height:150*t});e.add(i);var s=Ti.UI.createImageView({image:hub.API.imagePath("send.png"),width:85,top:10,right:5});return e.add(s),e},s=require("ui/common/UserView");return newmessage_win=new s("Inbox"),self=i(),newmessage_win.addContent(self),thisWindow=newmessage_win.appwin,thisWindow}module.exports=NewMessageView