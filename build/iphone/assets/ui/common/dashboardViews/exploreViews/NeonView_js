function buildNeonView(e){var t=Ti.UI.createView({backgroundColor:hub.API.customBgColor,layout:"vertical",table:null}),n=Ti.UI.createView({top:0,height:100,layout:"horizontal"}),r=Ti.UI.createImageView({image:e.neon.avatarUrl,height:100,width:100,left:1,top:1}),i=Ti.UI.createView({top:0,left:5,height:100,layout:"vertical"});n.add(r,i);var s=Ti.UI.createLabel({top:0,height:20,left:0,font:{fontSize:14},text:e.neon.activityType});detail_location="";var o=e.formattedDescription;detail_category=e.neon.creatorDisplayName+" ("+e.neon.creatorUserType+", "+e.neon.creatorCountry+")";var u=Ti.UI.createLabel({top:0,height:40,left:0,font:{fontSize:14},text:e.neon.neonTitle}),a=Ti.UI.createLabel({top:0,height:30,left:0,font:{fontSize:14},text:detail_category});i.add(s,u,a);var f=Ti.UI.createView({top:0,layout:"vertical"}),l=Ti.UI.createView({top:0,height:30,layout:"horizontal"}),c=Ti.UI.createLabel({top:0,left:5,font:{fontSize:14},text:"Expires :"}),h=Ti.UI.createLabel({top:0,left:5,font:{fontSize:14},text:e.neon.expiresAt});l.add(c,h),o&&o!="<p></p>"?o='<div style = "font-size:14px">'+o+"</div>":o="<p></p>";var p=Ti.UI.createWebView({top:0,html:o});return e.neon.showExpiryDate&&f.add(l),f.add(p),t.add(n,f),t}function neonView(e){var t=require("ui/common/UserView");return win=new t("Explore"),self=buildNeonView(e),win.addContent(self),thisWindow=win.appwin,thisWindow}hub=require("hub"),module.exports=neonView