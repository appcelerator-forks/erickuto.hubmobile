function SelectorView(e,t){var n=require("ui/common/UserView");win=new n("Explore"),hub=require("hub");var r=[];t.getChoices(e,r);var i=Ti.UI.createView(),s=hub.API.user,o=s.getSelectedOptions(e);if(e=="free"){AutoComplete=require("ui/common/dashboardViews/exploreViews/AutoCompleteView");if(r.length>0){var u=new AutoComplete(r);i.add(u)}else{var a=Ti.UI.createLabel({text:"Error: Hub Mobile was unable to fetch tags. ",top:50});i.add(a)}}else if(r.length>0){MultiChoice=require("ui/common/tableViewMultiSelectRows");var f=new MultiChoice(r,o,e);i.add(f)}else{var a=Ti.UI.createLabel({text:"Loading "+e+"...",top:50});e=="groups"?a.text="There are no groups to choose from. Please select a community first or join one.":e=="countries"?a.text="There no countries to choose from. \nPlease select a region first.":e=="cities"&&(a.text="There no cities to choose from. \nPlease select a country first. Note that there may be countries without tagged cities, provinces or locations"),i.add(a)}return win.addContent(i),win.addOnCloseEvent("updateSizes"),thisWindow=win.appwin,thisWindow}module.exports=SelectorView