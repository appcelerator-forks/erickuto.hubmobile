function buildUserView(e){var t=Ti.UI.createView({backgroundColor:hub.API.customBgColor,layout:"vertical"}),n=Ti.UI.createView({top:0,height:100,layout:"horizontal"}),r=Ti.UI.createImageView({image:e.userMetaData.avatarUrl,height:100,width:100,left:1,top:1}),s=Ti.UI.createView({top:0,left:5,height:100,layout:"vertical"});n.add(r,s);var o=Ti.UI.createLabel({top:0,height:20,left:0,font:{fontSize:14},text:e.userMetaData.displayName}),u=Ti.UI.createLabel({top:0,height:20,left:0,font:{fontSize:14},text:e.userMetaData.userType}),a=Ti.UI.createLabel({top:0,height:20,left:0,font:{fontSize:14},text:e.userData.designation});s.add(o,u,a);var f=Ti.UI.createView({top:0,layout:"vertical"}),l=Ti.UI.createView({top:0,height:20,layout:"horizontal"}),c=Ti.UI.createLabel({top:0,left:0,font:{fontSize:14},text:"Located In:"}),h="";e.userData.city!=null&&(h+=e.userData.city),e.userData.country!=null&&(e.userData.city!=null&&(h+=" , "),h+=e.userData.country);var p=Ti.UI.createLabel({top:0,left:0,font:{fontSize:14},text:h});l.add(c,p);var d=Ti.UI.createView({top:0,height:20,layout:"horizontal"}),v=Ti.UI.createLabel({top:0,left:0,font:{fontSize:14},text:"Languages:"}),m="";for(i=0;i<e.languages.length;i++)m+=e.languages[i].name,i!=e.languages.length-1&&(m+=", ");var g=Ti.UI.createLabel({top:0,left:0,font:{fontSize:14},text:m});d.add(v,g),m!=""&&s.add(d),h!=""&&s.add(l);var y=e.userData.about;y&&y!="<p></p>"?y='<div style = "font-size:14px">'+y+"</div>":y="<p></p>";var b=Ti.UI.createWebView({top:0,html:y});return f.add(b),t.add(n,f),t}function userView(e){var t=require("ui/common/UserView");return win=new t("Explore"),self=buildUserView(e),win.addContent(self),thisWindow=win.appwin,thisWindow}hub=require("hub"),module.exports=userView