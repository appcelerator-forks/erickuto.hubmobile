function AboutView(e){var t=require("ui/common/UserView");win=new t,hub=require("hub");var n=hub.API.hsf,r=hub.API.wsf,i=Ti.UI.createView({backgroundColor:hub.API.customBgColor}),s=Ti.UI.createLabel({top:105,text:"My Offers",color:hub.API.customTextColor,height:40,font:{fontSize:28},left:35});return i.add(s),win.addContent(i),thisWindow=win.appwin,thisWindow}module.exports=AboutView