function AboutView(e){hub=require("hub");var t=hub.API.hsf,n=hub.API.wsf,r=(hub.API.app_width-350*n)/2,i=require("ui/common/UserView");win=new i;var s=hub.API.user.profile,o=Ti.UI.createView({backgroundColor:hub.API.customBgColor}),u=Ti.UI.createView({top:0,left:0,height:150,layout:"horizontal"}),a=Ti.UI.createImageView({top:5*t,width:100,left:5*n,image:hub.API.imagePath("einstein.png")}),f=Ti.UI.createView({top:5*t,width:hub.API.app_width-110,left:5*t,layout:"vertical"}),l=s.first_name+" "+s.last_name;f.add(Ti.UI.createLabel({top:5*t,text:l,height:25,left:5*t,color:"black",font:{fontSize:20,fontWeight:"bold"}})),f.add(Ti.UI.createLabel({top:5*t,text:hub.API.userTypes[s.user_type_id],left:5*t,height:20,font:{fontSize:15},color:"blue"})),u.add(a),u.add(f),o.add(u);var c=Ti.UI.createView({top:150,left:5,height:"auto",width:hub.API.app_width-10,layout:"horizontal"}),h=Ti.UI.createLabel({top:5,left:5,height:"auto",width:"auto",text:s.about});c.add(h),o.add(c);var p=Ti.UI.createView({top:10,left:5,height:"auto",width:hub.API.app_width-10,layout:"horizontal"}),d=function(e){var t="ui/common/dashboardViews/profileViews/"+e;PageView=require(t),pageView=new PageView,hub.API.openWindow(pageView)},v=function(e){var r=e+".png",i=Ti.UI.createImageView({left:10*n,top:5*t,width:60,image:hub.API.imagePath(r)});return profile_link={change_makers:s.changemakers_profile_url,linked_in:s.linkedin_profile_url,youtube:s.profile_video_url,skype:'"skype:"skypeName'},i.addEventListener("click",function(){Ti.Platform.openURL(profile_link[e]),Ti.API.info(profile_link[e])}),i};icons=["change_makers","linked_in","skype","youtube"];for(var m=0;m<icons.length;m++)icon=v(icons[m]),p.add(icon);return c.add(p),win.addContent(o),thisWindow=win.appwin,thisWindow}module.exports=AboutView