function ProfileView(e){hub=require("hub");var t=hub.API.hsf,n=hub.API.wsf,r=(hub.API.app_width-350*n)/2,i=require("ui/common/UserView");win=new i("Profile");var s=hub.API.user.profile,o=Ti.UI.createView({backgroundColor:hub.API.customBgColor}),u=Ti.UI.createView({top:0,left:0,height:150,layout:"horizontal"}),a=Ti.UI.createImageView({top:5*t,width:100,left:5*n,image:hub.API.imagePath("einstein.png")}),f=Ti.UI.createView({top:5*t,width:hub.API.app_width-110,left:5*t,layout:"vertical"}),l=s.first_name+" "+s.last_name;f.add(Ti.UI.createLabel({top:5*t,text:l,height:25,left:5*t,color:"black",font:{fontSize:20,fontWeight:"bold"}})),f.add(Ti.UI.createLabel({top:5*t,text:hub.API.userTypes[s.user_type_id],left:5*t,height:20,font:{fontSize:15},color:"blue"}));var c=Ti.UI.createView({top:5*t,left:5*t,height:20,layout:"horizontal"});c.add(Ti.UI.createLabel({top:0,text:"Located in: ",color:"black",font:{fontSize:15,fontWeight:"bold"}})),c.add(Ti.UI.createLabel({top:0,color:"black",text:s.city,font:{fontSize:15}}));var h=Ti.UI.createView({top:5*t,left:5*t,height:20,layout:"horizontal"});h.add(Ti.UI.createLabel({top:0,text:"Languages: ",color:"black",font:{fontSize:15,fontWeight:"bold"}})),h.add(Ti.UI.createLabel({top:0,text:"English, Swahili",color:"black",font:{fontSize:15}})),f.add(c),f.add(h),u.add(a),u.add(f),o.add(u);var p=Ti.UI.createView({top:150,left:5,height:"auto",width:hub.API.app_width-10,layout:"horizontal"}),d=function(e){var t="ui/common/dashboardViews/profileViews/"+e;PageView=require(t),pageView=new PageView,hub.API.openWindow(pageView)},v=function(e){var r="my_"+e+".png",i=Ti.UI.createImageView({left:40*n,top:15*t,width:70,image:hub.API.imagePath(r)});return i.addEventListener("click",function(){d(e)}),i};icons=["About","Activity","Information","Offers","Needs","Events","Tags","Followers","Following"];for(var m=0;m<icons.length;m++)icon=v(icons[m]),p.add(icon);return o.add(p),win.addContent(o),thisWindow=win.appwin,thisWindow}module.exports=ProfileView