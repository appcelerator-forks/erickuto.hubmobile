logoutUser=function(e,t,n,r){var i=Ti.Network.createHTTPClient(),s=Ti.App.getRemoteURL("api/mobile/tokens.json");Ti.API.info("url is: "+s),r=r||0;var o={email:t,password:n};Ti.API.info(" user "+t+" password "+n),i.open("POST",s),i.onload=function(s){var o=this.responseText;if(i===null||o===null){if(r<3){r++,Ti.API.info("trying .. "+r),authenticateUser(e,t,n,r);return}alert("Error connecting to internet. Please make sure you have a connection and try restarting Hub"),e.error&&e.error();return}var u=JSON.parse(o);Ti.API.info(u),u.logged==="true"?e.success&&e.success(u.token):e.failure&&e.failure()},i.onerror=function(t){e.error&&e.error()},e.start&&e.start(),i.send(o)},module.exports=logoutUser