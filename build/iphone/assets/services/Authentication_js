hub=require("hub"),authenticateUser=function(e,t,n){var r={email:t,password:n};Connection=require("services/Connection");var i=new Connection({start:function(){e.start&&e.start()},error:function(){e.error&&e.error()},success:function(t){t.logged==="true"?e.success&&(hub.API.user.setAuthToken(t.token),e.success()):e.failure&&e.failure()}},"tokens",r,"POST")},module.exports=authenticateUser