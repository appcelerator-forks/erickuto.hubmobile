function SearchResults(e,t,n){Connection=require("services/Connection");var r=new Connection({start:function(){n.start&&n.start()},error:function(){n.error&&n.error()},success:function(t){page=t.page,page==0&&(results=[]),t.hasMore==1?hasMore=!0:hasMore=!1;if(e==="activities/count")counts=t;else{_results=t.results;for(i=0;i<_results.length;i++)results.push(_results[i])}n.success()}},e,[],"GET",t);this.getResults=function(e){for(i=0;i<results.length;i++)e.push(results[i])},this.getNeonPath=function(e){return neon=results[e],neon.activityType==="Person"?neon.userUrl:neon.neonUrl},this.getNeonType=function(e){return results[e].activityType},this.getCounts=function(e){return counts[e]},this.hasMore=function(){return hasMore},this.getPage=function(){return page},this.changeReadyState=function(e){isReady=e}}var results=[],counts={},hasMore=!1,page=0;module.exports=SearchResults