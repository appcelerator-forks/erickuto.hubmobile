hub=require("hub"),connectHub=function(e,t,n,r,i){var s=Ti.Network.createHTTPClient(),o="api/mobile/"+t+".json";t.substring(0,9)==="activities"&&(ulrText=o.replace("api/mobile",""));var u="",a=0;if(i!=null){for(var f=0;f<i.length;f++)key="key"+f,value="value"+f,f==0?u=u+"?"+i[f][key]+"="+i[f][value]:u=u+"&"+i[f][key]+"="+i[f][value],i[f][key]==="page"&&(a=i[f][value]);o+=u}var t=hub.API.getRemoteURL(o);Ti.API.info("Fetching from: ["+t+"]"),s.open(r,t),s.onload=function(t){var n=this.responseText,r=JSON.parse(n);r.page=a,e.success&&e.success(r)},s.onerror=function(t){e.error&&e.error()},e.start&&e.start(),s.send(n)},module.exports=connectHub