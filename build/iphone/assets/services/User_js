function getArray(e){switch(e){case"people":return followed_people;case"communities":return followed_communities;case"target":return followed_population;case"groups":return followed_groups;case"fields":return followed_fields;case"free":return followed_free_tags;case"regions":return followed_regions;case"countries":return followed_countries;case"cities":return followed_cities;default:return[]}}hub=require("hub");var first_name="Eric",last_name="Kuto",followed_people=[],followed_population=[],followed_regions=[],followed_groups=[],followed_communities=[],followed_countries=[],followed_fields=[],followed_free_tags=[],followed_cities=[],authToken=null;userFunction=function(){this.getSelectedOptions=function(e){return getArray(e)},this.getAll=function(e){var t=["people","communities","groups","regions","countries","cities","fields","target","free"],n=0;for(var r=0;r<t.length;r++){thisArray=getArray(t[r]);if(thisArray.length>0){var i=[];hub.API.explorer.getChoices(t[r],i);for(var s=0;s<thisArray.length;s++){var o={};keyText="key"+n,valueText="value"+n,t[r]==="free"?(o[keyText]="tags",o[valueText]=thisArray[s]):t[r]==="people"?(o[keyText]="user_types",o[valueText]=i[thisArray[s]]):(t[r]==="countries"?o[keyText]="locations_of_work":t[r]==="cities"?o[keyText]="locations":t[r]==="fields"?o[keyText]="fields_of_work":o[keyText]=t[r],o[valueText]=i[thisArray[s]]),e.push(o),n++}}}},this.clearGroupOptions=function(){followed_groups=[]},this.clearCountriesOptions=function(){followed_countries=[]},this.clearSubLocationsOptions=function(){followed_cities=[]},this.setAuthToken=function(e){authToken=e},this.getAuthToken=function(){return authToken},this.getSelectedSize=function(e){return getArray(e).length}},module.exports=userFunction